<div class="left">
<form  action="/newPage/{{city}}" method="POST">


<select class="select-statement" name="location_name">
    {{#each locations}}
        <option value ={{category_name}}>
             {{category_name}}
        </option>
    {{/each}}
</select>
<button type='submit'>Submit</button>
</form>


<ul  class="list">
    {{#each results }}
    <li>
  <a target="_blank" href='http://www.google.com/maps/search/?api=1&query={{latitude}},{{longitude}}'>
  
  {{location_name}}</a>
    </li>
    {{/each}}
</ul> 

